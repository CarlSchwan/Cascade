QWidget {
    background-color: #181b1e;
    color: #f1f1f1;
    font: 14px "Open Sans";
    border: 0px;
    margin: 0px;
    padding: 0px;
}
QWidget#NodeBase {
    background-color: #181b1e;
}
QWidget#ViewerStatusBar {
    background-color: #282d31;
    border-color: #626971;
    border-style: solid;
    border-width: 2px;
}
VulkanView {
    margin: 0px;
    border: 0px;
}

QListView {
    background-color: #1d2024;
    font: 14px "Open Sans";
    color: #f1f1f1;
    border: none;
}
QListView::item:selected {
    background-color: #233443;
    color: #f1f1f1;
}

QPlainTextEdit {
    background-color: #282d31;
    color: #f1f1f1;
}

ControlsWidget {
    background-color: #282d31;
    color: #f1f1f1;
}

/* Horizontal and Vertical Lines */
QFrame[frameShape="4"],
QFrame[frameShape="5"] {
    background-color: #282d31;
    border: node;
    max-height: 1px;
}

QPushButton {
    background-color: #282d31;
    color: #f1f1f1;
    border: none;
    border-radius: 2px;
}
QPushButton:hover {
    background-color: #626971;
}
QPushButton:disabled {
    color: #9299a1;
    background-color: #1d2024;
}

NodeInput, NodeOutput {
    border-radius: 1px;
}

QScrollBar:vertical {
    border: 0px;
    width: 15px;
    background: #1d2024;
}
QScrollBar::handle:vertical {
    background: #626971;
}
QScrollBar::up-arrow:vertical, QScrollBar::down-arrow:vertical {
     border: 0px;
     background: none;
     color: none;
}
QScrollBar::add-line:vertical {
      border: none;
      background: none;
}
QScrollBar::sub-line:vertical {
      border: none;
      background: none;
}

QScrollBar:horizontal {
    border: 0px;
    height: 15px;
    background: #1d2024;
}
QScrollBar::handle:horizontal {
    background: #626971;
}
QScrollBar::up-arrow:horizontal, QScrollBar::down-arrow:horizontal {
     border: 0px;
     background: none;
     color: none;
 }
 QScrollBar::add-line:horizontal {
       border: none;
       background: none;
 }
 QScrollBar::sub-line:horizontal {
       border: none;
       background: none;
 }

QPlainTextEdit {
    font: 12px "Open Sans";
    color: #9299a1
}

